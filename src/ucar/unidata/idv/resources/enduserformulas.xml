         value="Grids"/>
      <category value="GRID-2D-*"/>
      <!--
      <category value="display:planviewcontour"/>
      <category value="display:planviewcontourfilled"/>
      <category value="display:planviewcolor"/>
      -->
    </categories>
    <operands/>
  </derived>
